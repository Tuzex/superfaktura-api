    "php": "^8.1",
    "beberlei/assert": "3.3.*",
    "symfony/options-resolver": "5.*|6.*"
  },
  "require-dev": {
    "phpstan/phpstan": "1.3.*",
    "phpstan/phpstan-phpunit": "1.0.*",
    "phpunit/phpunit": "9.5.*",
    "symfony/var-dumper": "6.*",
    "symplify/easy-coding-standard": "10.0.*"
  }
}
